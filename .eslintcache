 useEffect(()=>{\n        getItems.then((item)=>{\n            setGettedItem(item)\n        }).catch(error => console.log(error))\n        .finally(()=>{\n            setLoading(false);\n        });\n    },[]);\n\n    return(\n        loading ? <LoaderGif /> : <ItemDetail item={gettedItem}/>\n    )\n}\n\nexport default ItemDetailContainer;","/home/ceci/ceci/ecommerce/src/components/CartContext/CartContext.js",["97"],"/home/ceci/ceci/ecommerce/src/components/NavBar/CartWidget.js",[],"/home/ceci/ceci/ecommerce/src/components/Cart/Cart.js",["98","99"],"/home/ceci/ceci/ecommerce/src/components/loader-gif-style.js",[],"/home/ceci/ceci/ecommerce/src/components/ItemCount/CartCount.js",["100","101","102","103","104","105","106","107","108","109","110","111","112"],{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":45,"column":7,"nodeType":"119","endLine":45,"endColumn":19,"suggestions":"120"},{"ruleId":"121","severity":1,"message":"122","line":11,"column":35,"nodeType":"123","messageId":"124","endLine":11,"endColumn":43},{"ruleId":"121","severity":1,"message":"125","line":11,"column":45,"nodeType":"123","messageId":"124","endLine":11,"endColumn":56},{"ruleId":"121","severity":1,"message":"126","line":11,"column":67,"nodeType":"123","messageId":"124","endLine":11,"endColumn":77},{"ruleId":"121","severity":1,"message":"127","line":11,"column":79,"nodeType":"123","messageId":"124","endLine":11,"endColumn":84},{"ruleId":"121","severity":1,"message":"128","line":11,"column":86,"nodeType":"123","messageId":"124","endLine":11,"endColumn":94},{"ruleId":"121","severity":1,"message":"129","line":9,"column":13,"nodeType":"123","messageId":"124","endLine":9,"endColumn":19},{"ruleId":"117","severity":1,"message":"130","line":25,"column":7,"nodeType":"119","endLine":25,"endColumn":9,"suggestions":"131"},{"ruleId":"121","severity":1,"message":"132","line":1,"column":26,"nodeType":"123","messageId":"124","endLine":1,"